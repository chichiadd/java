//青蛙跳台阶
public class JUMP {
    public  static void main(String args[]){
        int count = 0;
        int number = 5;
        System.out.println(jump(number));
    }
    public static int jump (int n){
        if(n == 1)return 1;
        if(n == 2)return 2;
        return (jump(n -1) + jump(n -2));
    }

